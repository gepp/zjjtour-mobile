/*全景张家界*/
.panorama{width: 1200px; margin: 0 auto;}
.panorama>ul>li{float: left; width: 25%; position: relative; overflow: hidden; vertical-align: middle;}
.panorama>ul>li>.mask{height: 0; width: 100%; position: absolute; bottom: 0; background: rgba(255,255,255,0.7); overflow: hidden; text-align: center;}
.panorama>ul>li:hover>.mask{height: 100%; bottom: 0; left: 0;}
.panorama>ul>li>img{width: 100%;}
.panorama>ul>li:hover>img{ transform: scale(1.5);}
.panorama>ul>li>.mask>h4{font-weight: 100; color: #4a4a4a; display: block; line-height: 50px; margin-top: 20%;}
.panorama>ul>li>.mask>button{ color:#00BB9E ; border: 1px solid #00BB9E; padding: 0 10px; line-height: 36px; height: 36px; background: none; border-radius: 18px; font-size: 16px; cursor: pointer;}
.panorama>ul>li>.mask>button:hover{ background:#00BB9E; color: #fff;} 
/*畅游张家界*/
.index-swim{ width: 1200px; margin: 60px auto;}
.index-swim>.index-swim-left{ width:40%; float: left; margin-top: 10%;}
.index-swim>ul{ width: 60%; float: left; margin:0;}
.index-swim>ul>li{padding: 20px; border-bottom: 1px solid #eee;}
.index-swim>ul>li>h4{ color: #00BB9E; line-height: 50px;}
.index-swim>ul>li>p{line-height: 30px;}
.index-swim>ul>li:hover{padding: 20px 0 20px 40px; cursor: pointer;}
.index-swim>ul>li:hover>p{color: #333;}
/*娱乐张家界*/
.entertainment{position: relative; width: 1200px; margin: 0 auto;}
.entertainment>.entertainment-content{width: 50%; float: left; position:relative; overflow: hidden;}
.entertainment>.entertainment-content>img{width:100%;}
.entertainment>.entertainment-content:hover>img{transform: scale(1.2);}
.entertainment>.entertainment-content>.mask{width: 60%; height: 60%; background: rgba(0,0,0,0.6); position:absolute; top: 20%; left: 20%;}
.entertainment>.entertainment-content>.mask>button{display: block; font-size: 30px;  height: 60px; margin-top: 5%; margin-left:calc(50% - 91px);margin-left:-webkit-calc(50% - 91px); margin-left: -moz-calc(50% - 91px); border-radius: 30px; padding:0 30px; color: #fff; background: none;  border: 1px solid #fff; line-height: 60px;}
.entertainment>.entertainment-content>.mask>button:hover{color: #fff; background: #00BB9E; border:1px solid #00BB9E;}
.entertainment>.entertainment-content>.mask>p{display: block; margin-top: 20px; line-height: 30px; color: #fff; width:90%; margin-left: 5%; text-align: center;}
.entertainment>.index-title{ position:absolute; top: 15%; z-index: 2; width: 210px; left: calc(50% - 105px);left: -webkit-calc(50% - 105px);left: -moz-calc(50% - 105px);}
.entertainment>.index-title>h2{ color: #fff;}
/*休闲张家界*/
.leisure{width: 1200px; margin: 0 auto;}
.leisure>.delicacy,.leisure>.hotel{width: 25%; float: left; height: 600px; overflow: hidden;}
.leisure>.spots,.leisure>.gate{width: 50%; float: left; height: 300px; overflow: hidden;}
.leisure>.spots>.leisure-top{z-index: 3;}
.leisure>.spots>.leisure-top,.leisure>.spots>.leisure-bottom,.leisure>.gate>.leisure-top,.leisure>.gate>.leisure-bottom{ width: 50%; float: left;}
.leisure-top,.leisure-bottom{position: relative; height: 300px; background: #f5f5f5;}
.delicacy{position:relative;}
.delicacy>.mask{position: absolute; z-index: 2; bottom: 0; width: 70%; left: 15%; padding-bottom: 5%;}
.delicacy>.mask>h4{ color: #fff; line-height: 50px; border-bottom: 1px solid #fff;}
.delicacy>.mask>p{line-height: 30px; margin-top: 20px; color: #fff; padding: 0;}
.delicacy>img{ width: 100%; height: 600px; overflow: hidden;}
.delicacy:hover>img{ transform: scale(1.2);}
.leisure>div img{width:100%; height: 100%;}
.leisure>div>div>h5{line-height: 50px; padding:20px 20px 0 20px;}
.leisure>div>div>p{line-height: 30px; padding:0 20px 20px 20px;}
.leisure>div>div>p>a{ color: #00bb9e;}
.hotel:hover img,.spots:hover img,.gate:hover img{ transform: scale(1.2);}
.hotel:hover h5,.hotel:hover a,.hotel:hover p,.spots:hover h5,.spots:hover a,.spots:hover p,.gate:hover h5,.gate:hover a,.gate:hover p{color: #fff;}
.hotel:hover .leisure-bottom,.spots:hover .leisure-top,.gate:hover .leisure-bottom{background: #00bb9e; z-index: 3;}
.hotel>.leisure-bottom>i{ width: 0px; height: 0px;  border-bottom: 24px solid #f5f5f5; border-left: 24px solid  transparent; border-right: 24px solid transparent; position:absolute;top: -24px; left: calc(50% - 24px); left: -webkit-calc(50% - 24px); left: -moz-calc(50% - 24px);}
.hotel:hover>.leisure-bottom>i{border-bottom: 24px solid #00bb9e; border-left: 24px solid  transparent; border-right: 24px solid transparent;}

.spots>.leisure-top>i{z-index: 2; width: 0px; height: 0px;  border-left: 24px solid #f5f5f5; border-top: 24px solid  transparent; border-bottom: 24px solid transparent; position:absolute;right: -24px; top: calc(50% - 24px); top: -webkit-calc(50% - 24px); top: -moz-calc(50% - 24px);}
.spots:hover>.leisure-top>i{border-left: 24px solid #00bb9e;}

.gate>.leisure-bottom>i{ z-index: 2; width: 0px; height: 0px;  border-right: 24px solid #f5f5f5; border-top: 24px solid  transparent; border-bottom: 24px solid transparent; position:absolute;left: -24px; top: calc(50% - 24px); top: -webkit-calc(50% - 24px); top: -moz-calc(50% - 24px);}
.gate:hover>.leisure-bottom>i{border-right: 24px solid #00bb9e;}

/*底部*/
.footer{width: 100%; min-width: 1200px; background: #F5F5F5; border-top: 3px solid #00BB9E;}
.footer>h4{ margin: 40px auto; line-height: 40px; color: #4A4A4A;width: 1200px;}
.footer-link{ margin: 0 auto;width: 1200px;}
.footer-link>ul{ margin: 0px -2%;}
.footer-link>ul>li{float: left; margin: 2%; width: 16%;}
.footer-link>ul>li>select{ width: 100%; padding: 5px; box-sizing: border-box; display: block; border: 1px solid #ccc; background: none; font-size: 14px; color: #999; outline: none; height: 30px;}
.footer-item-list{margin: 20px auto;width: 1200px;}
.footer-item-list>.footer-item{float: left; margin-left: 3%; width: 120px; text-align: center; cursor: pointer;}
.footer-item-list>.footer-item>i{display: block; font-size: 80px; color: #00BB9E; margin-bottom: 20px;}
.footer-item-list>.footer-item>span{ font-size: 24px; color: #00BB9E;}
.footer-item-list>.footer-item:first-child{ margin-left: 0;}
.footer-item-list>.footer-item:nth-child(3){margin-top: 7px;}
.footer-item-list>.footer-item:nth-child(3)>i{margin-bottom: 13px;}
.footer-item-list>.footer-item:nth-child(4)>img{ width: 100%; margin-bottom: 20px;}
.footer-item-list>.footer-item:nth-child(5),.footer-item-list>.footer-item:nth-child(4){float: right; width: 100px;}
.footer-item-list>.footer-item:nth-child(5)>i{ height: 100px; width: 100px; background: #00BB9E; text-align: center; line-height: 100px; color: #fff;}
.footer-item-list>.footer-item:nth-child(5)>span,.footer-item-list>.footer-item:nth-child(4)>span{ font-size: 14px;  color: #666;}
.footer-img-list>img{float: left; margin: 20px; cursor: pointer;}
.footer-img-list{width: 800px; margin: 0 auto 20px auto;}
.copyright{ width: 100%; background: #00BB9E; color: #fff; text-align: center; line-height: 60px;}
/*悬浮 */
#tbox{ position:fixed; display: none; z-index: 5;}
.taoba{display:block; background:#00BB9E; padding: 10px 15px; font-size: 16px; border-bottom: 1px solid rgba(255,255,255,0.2); color: #4a4a4a; line-height: 30px;position:relative;cursor:pointer}
.taoba>ul{position: absolute; right: 62px; top: 0;width:150px; display: none; text-align: center;border-bottom-right-radius: 5px; border-bottom-left-radius: 5px;border-top-left-radius: 5px; overflow: hidden;}
.taoba:hover>ul{ display: block; }
.taoba>ul>li{padding: 10px 15px;background:#35DCC2; border-bottom: 1px solid rgba(255,255,255,0.2); color: #4a4a4a;}
.taoba>ul>li:hover{color:#fff ;}
.taoba>i{position: absolute; left: 2px; top: 11px; font-size: 12px;}
.taoba>ul>li>span{display: block;}
.taoba>ul>li>i{font-size: 30px;}
.taoba.active{background:#666; text-decoration: none; color: #fff;}
.taoba:hover{background:#35DCC2; text-decoration: none;}
.taoba:first-child{border-top-right-radius: 5px; border-top-left-radius: 5px;}
.taoba:nth-child(8){border-bottom-right-radius: 5px; border-bottom-left-radius: 5px;}
#gotop,#topcontrol{display:block; padding: 0px 9px; overflow: hidden; color: #fff; background: #00BB9E;  border-radius: 5px;margin-top: 5px;position:absolute;  cursor:pointer;} 
#gotop>i,#topcontrol i{ font-size: 44px;}
#gotop:hover,#topcontrol:hover{background: #dedede;}
#gotop:hover>i,#topcontrol:hover i{color: #666;}


/*视频弹窗*/
.box{width:100%;height:100%;background:rgba(0,0,0,0.5);position:fixed;z-index:9999;display:none;}
.hint{/*width: 800px;height: 500px;*/margin:0 auto;border: 10px solid rgba(0, 0, 0, 0.8);background:#fff;position:fixed;top:50%;left:50%;margin-left: -405px;margin-top: -255px;z-index:99999;display:none;}
