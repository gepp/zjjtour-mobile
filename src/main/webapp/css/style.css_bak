.hint2{line-height: 50px;color: #FFFFFF;font-size: 18px;padding-left: 20px;}
.hint3{width:40px;height:40px;line-height: 40px; text-align: center; position:absolute;top:5px;right:5px; cursor: pointer;}
.hint3>i{font-size: 24px; color: #fff;}
.hint3:hover>i{ color: #4A4A4A;}


/*全景列表页面*/
.page-breadcrumbs{ /*height: 60px;*/ width: 100%; background: #F5F5F5; line-height: 60px;}
.page-breadcrumbs>ul{width: 1200px; margin: 0 auto;}
.page-breadcrumbs>ul>span{ float: left; color: #00BB9E; font-size: 14px;}
.page-breadcrumbs>ul>li{ float: left;}
.page-breadcrumbs>ul>li>a:hover{color: #00BB9E;}
.page-breadcrumbs>ul>li+li:before{content: "\e615"; font-family:"iconfont" !important;
  font-size:12px;
  font-style:normal;margin: 0 5px;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;}
  .page-breadcrumbs>ul>li>a{ color:#4a4a4a ;}
.list-title{margin-top: 60px; border-bottom: 1px solid #00BB9E;}
.list-title>ul{width: 1200px; margin: 0 auto;}
.list-title>ul>li{ width: 145px; float: left; line-height: 70px; height: 70px; margin-bottom: -1px; text-align: center; font-size: 24px; color: #4A4A4A; cursor: pointer;}
.list-title>ul>li.active{ color: #00BB9E; border: 1px solid #00BB9E; border-bottom: transparent; background: #fff; border-top-left-radius: 5px; border-top-right-radius: 5px;}
.list-title>ul>li:hover{ color: #00BB9E;}
.list-label{ width: 1200px; margin: 60px auto 20px auto; position: relative;}
.list-label>span{position: absolute; right: 0px; width: 60px; height: 60px; top: 0px; line-height: 60px; background: #00BB9E; text-align: center; border: 1px solid #00BB9E;  box-sizing: border-box; color: #fff;}
.list-label>span:hover{border: 1px solid #14A0DB; background: #14A0DB; box-sizing: border-box; cursor: pointer;}.list-label>span>i{font-size: 30px;}
.list-label>span:hover>i{font-size: 30px; color: #fff;}


.item-list,.activity-item-list {width: 1200px;margin: 60px auto;}
.item-list>ul,.activity-item-list>ul {margin: 0 -15px;}
/*.item-list>ul>li:first-child,.activity-item-list>ul>li:nth-child(6n-5){width: calc(100% - (100% / 3) - 30px);}*/
