* {padding: 0;margin: 0;}

input {outline: none}

iframe,
img {display: block;}
button {cursor: pointer;outline: none}

html,
body {font-family: "微软雅黑"; background-image:url(about:blank);background-attachment:fixed;}
h1 {font-size: 48px;}
h2 {font-size: 40px;}
h3 {font-size: 36px;}
h4 {font-size: 24px;}
h5 {font-size: 18px;}
h6 {font-size: 16px;}

ul,
li,
ol {list-style: none;}

.clr:before,
.clr:after {content: "";display: block;clear: both;}
::-webkit-input-placeholder { /* WebKit browsers */
    color:    #999;
    font-size: 14px;
}
:-moz-placeholder { /* Mozilla Firefox 4 to 18 */
    color:    #999;
    font-size: 14px;
}
::-moz-placeholder { /* Mozilla Firefox 19+ */
    color:    #999;
    font-size: 14px;
}
:-ms-input-placeholder { /* Internet Explorer 10+ */
    color:    #999;
    font-size: 14px;
}

body {min-width: 1200px;font-size: 14px;color: #999;}

a {text-decoration: none;}

*:hover {transition: all linear .2s !important;-webkit-transition: all linear .2s !important;-moz-transition: all linear .2s !important;-ms-transition: all linear .2s !important;}
.transition,img,svg,button,p{transition: all linear .2s !important;-webkit-transition: all linear .2s !important;-moz-transition: all linear .2s !important;-ms-transition: all linear .2s !important;}
.navbar-fixed-top {position: fixed;height: 120px;background: rgba(255, 255, 255, 0.8);box-shadow: 0 1px 0px rgba(243, 243, 243, 0.8);top: 0;left: 0;z-index: 999;width: 100%;min-width: 1200px;}
.navbar-fixed-top>.logo {float: left;}
.navbar-fixed-top>.logo>img {height: 90px;margin: 15px;display: block;}
.logo-title {float: left;padding-top: 30px;}
.logo-title>h4 {color: #14A0DB;line-height: 30px;}
.logo-title>h6 {color: #333;line-height: 30px;}
.logo-right {float: right;  margin-right: 20px;}
.logo-right-top {padding: 10px;}
.logo-right-top>.search {float: right;position: relative;margin: 4px;}
.logo-right-top>.weather {float: right;margin-top: 8px;}
.logo-right-top>.login {float: right;}
.logo-right-top>.login>a {display: block;float: left;margin: 0 5px;line-height: 36px;color: #9b9b9b;font-size: 12px;}
.logo-right-top>.login>a:hover {color: #00BB9E;}
.logo-right-top>.search>input {height: 28px;width: 200px;border-radius: 14px;border: 1px solid #9b9b9b;padding: 0 10px;background: none;}
.logo-right-top>.search>input:focus {border: 1px solid #00BB9E}
.logo-right-top>.search>button {border: none;background: none;position: absolute;color: #909090;top: 0;right: 0;height: 28px;width: 30px;text-align: center;}
.logo-right-top>.search>button:hover {color: #00BB9E;}
.nav>li {float: left;margin: 6px 0;transform: skew(-30deg);transition: all linear .2s !important;-webkit-transition: all linear .2s !important;-moz-transition: all linear .2s !important;-ms-transition: all linear .2s !important;}
.nav>li>.normal>a {font-size: 16px;color: #333;padding: 0 30px;font-weight: 600px;line-height: 40px;height: 40px;display: block;}
.nav>li>.normal {transform: skew(30deg);}
.nav>li:hover,.nav>li.active {background: #00BB9E;}
.nav>li:hover>.normal>a,.nav>li.active>.normal>a {color: #FFFFFF;}
.item {width: 1200px;margin: 20px auto;}
.item>ul {margin: 0 -15px;}
.item>ul>li {display: block;float: left;width: calc(100% / 4 - 30px);margin: 0 15px;cursor: pointer;}
.item>ul>li>.img-content {width: 100%;position: relative;overflow: hidden;}
.item>ul>li>.img-content>img {width: 100%; height: 135px;}
.item>ul>li>.img-content>span {position: absolute;bottom: 0px;left: 0;padding: 0 0 0 5px;width: 100%;height: 0px;line-height: 40px;color: #ddd;background: rgba(0, 0, 0, 0.7);transition: all linear .2s !important;-webkit-transition: all linear .2s !important;-moz-transition: all linear .2s !important;-ms-transition: all linear .2s !important;}
.item>ul>li>h5 {font-size: 20px;color: #666;height: 60px;margin: 10px 0 0 5px;}
.item>ul>li:hover {background: #00BB9E;}
.item>ul>li:hover>.img-content>span {height: 40px;}
.item>ul>li:hover>.img-content>img {transform: scale(1.2);}
.item>ul>li:hover>h5 {color: #fff;}
.navigation-down {position: absolute;bottom: 0;left: 0px;width: 100%;}
.navigation-down .nav-down-menu {width: 100%;margin: 0;position: absolute;top: 0px;background: rgba(255, 255, 255, 0.9);}
.navigation-down .nav-down-menu .navigation-down-inner {margin: auto;width: 1200px;position: relative}
.navigation-down .nav-down-menu dl {float: left;margin: 18px 80px 18px 0}
.navigation-down .menu-1 dl {margin: 20px 80px 25px 0}
.navigation-down .menu-1 dt {font: normal 16px "microsoft yahei";color: #61789e;padding-bottom: 10px;border-bottom: 1px solid #61789e;margin-bottom: 10px}
.navigation-down .menu-1 dd a {color: #fff;font: normal 14px/30px "microsoft yahei"}
.navigation-down .menu-1 dd a:hover {color: #60aff6}
.navigation-down .menu-2 dd a,
.navigation-down .menu-3 dd a {color: #fff;font: normal 16px "microsoft yahei"}

/*焦点图*/

.index_focus {position: relative;width: 100%; min-width:1200px;height: 600px;margin: 0 auto;overflow: hidden;}
.index_focus .bd li {  display: none;position: absolute;left: 0;top: 0;}
.index_focus_post {z-index: 10;}
.index_focus .pic {height: auto;display: block;}
.index_focus .pic:hover {text-decoration: none;}
.index_focus .slide_nav {position: absolute;left: 45%;bottom:2%;
}
.index_focus .slide_nav a {cursor:pointer;float:left;font-size:28px;font-family: arial;color: rgba(255,255,255,0.7);padding:5px 10px;}
.index_focus .slide_nav li:hover,.index_focus .slide_nav .on{text-decoration: none;color:rgba(0,187,158,0.9);}
.index_focus_pre,.index_focus_next {display: none;position: absolute;top: 50%;margin-top: -36px;width: 72px;height: 72px;/*text-indent: 100%;*//*white-space: nowrap;*/overflow: hidden;z-index: 10;/*background: url(../images/ico_sliding.png) no-repeat;*/}
.index_focus_pre>i,.index_focus_next>i{ font-size: 60px; color: rgba(255,255,255,0.8);}
.index_focus_pre {left: 30px;background-position: 0 0;}
.index_focus_pre:hover>i {
   color: rgba(0,187,158,0.8);}
.index_focus_next {right: 30px;/*background-position: 0 -72px;*/}
.index_focus_next:hover>i {color: rgba(0,187,158,0.8);}
.index_focus_info {position: absolute;left: 10%;bottom: 10%;width: 800px;height: 200px;padding-bottom: 10px;color: #fff;text-decoration: none;cursor: pointer;font-family: "\5fae\8f6f\96c5\9ed1";-moz-text-shadow: 1px 1px 1px rgba(0, 0, 0, 0.6);-webkit-text-shadow: 1px 1px 1px rgba(0, 0, 0, 0.6);text-shadow: 1px 1px 1px rgba(0, 0, 0, 0.6);background: url("../images/index_focus_bg.png") no-repeat;_background: none;}
.index_focus_info h3 {font-size: 36px;line-height: 70px;font-family: \5fae\8f6f\96c5\9ed1, \u9ed1\u4f53, "Lucida Grande";display:none;}
.index_focus_info .text {width: 500px;line-height: 24px;font-size: 14px;word-wrap: break-word;overflow: hidden;display:none;}

/*首页大标题*/
.index-title{width: 425px; margin: 100px auto;}
.index-title>h2{ float: left; line-height: 60px; color: #4a4a4a; position: relative; cursor: pointer;}
.index-title>h2>i{ font-size: 100px; position: absolute; top: 0; left: 0; color: #00BB9E;}
.index-title>h2>span{ color: #00BB9E; margin-right: 10px;}
.index-title:hover svg{transform: rotate(-90deg);}
.index-title>.more{ font-size: 30px;  height: 60px; border-radius: 30px; padding:0 30px; color: #00BB9E; float: left; border: 1px solid #00BB9E; line-height: 60px; margin-left: 30px;}
.index-title>.more:hover{ background: #00BB9E; color: #fff; cursor: pointer;}


/*视觉张家界*/
.slider a {float: left;display: inline;position: relative;cursor: pointer;}
.slider a>.title{position: absolute;bottom: 0;height: 60px;line-height: 60px;color: #fff; width: 100%; padding-left:10px ; text-align: left; background: rgba(0,0,0,0.5);}
.slider a>i{position: absolute;font-size: 100px;width: 100%;text-align: center; left: 0; bottom: calc(50% - 30px);bottom: -moz-calc(50% - 30px);bottom: -webkit-calc(50% - 30px);height: 100px;color: rgba(0,187,158,0.6);}
.slider a:hover>i{color: rgba(0,0,0,0.6);}
.slider {width:100%;min-width:980px;overflow: hidden;height:520px;position:relative;background:#fff;text-align:center;
			}
			.slider .bd {width:980px;position:absolute;left:50%;margin-left:-490px
			}
			.slider .bd li {width:980px;overflow:hidden;
			}
			.slider .bd li img {display:block;width:980px;height:480px;
			}
			.slider .tempWrap {overflow:visible !important
			}
			.slider .tempWrap ul {margin-left:-980px !important;
			}
			.slider .hd {position:absolute;width:100%;left:0;z-index:1;height:8px;bottom:10px;text-align:center;
			}
			.slider .hd li {display:inline-block;*display:inline;zoom:1;width:15px;height:15px;line-height:99px;overflow:hidden;background:rgba(200,200,200,1);margin:0 5px;cursor:pointer;border-radius: 50%;
			}
			.slider .hd li.on {background:rgba(0, 187, 158, 1);
			}
			.slider .pnBtn {position:absolute;z-index:1;top:0;width:100%;height:480px;cursor:pointer;
			}
			.slider .prev {left:-50%;margin-left:-490px;
